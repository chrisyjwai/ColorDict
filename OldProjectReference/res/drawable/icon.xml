<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="300dp"
    android:width="300dp"
    android:viewportHeight="1600"
    android:viewportWidth="1600" >

    <group
        android:name="background"
        android:pivotX="800"
        android:pivotY="800"
        android:rotation="0" >
        <path
