            android:fillColor="@color/colorIconBackground"
            android:pathData="M0,800 a800,800 0 0 0 1600,0z " />
    </group>

    <group
        android:name="anger"
        android:pivotX="800"
        android:pivotY="800"
        android:rotation="0" >
        <path
            android:name="anger_slice"
            android:fillColor="@color/colorAnger"
            android:pathData="@string/path_slice_icon" />
    </group>
    <group
        android:name="disgust"
        android:pivotX="800"
        android:pivotY="800"
